{
  "name": "react-peek",
  "version": "1.0.5",
  "description": "Gather data by inspecting react components.",
  "main": "lib/prop-types.js",
  "keywords": [
    "react",
    "prop-types",
    "props",
    "data",
    "docs",
    "documentation"
  ],
  "author": "Oliver Gupte",
  "repository": {
    "type": "git",
    "url": "https://github.com/ogupte/react-peek.git"
  },
  "license": "MIT",
  "scripts": {
    "babel-prop-types": "babel src -d lib",
    "prepublish": "npm run babel-prop-types",
    "test": "jest",
    "watch": "jest --watch",
    "cover": "jest --coverage",
    "release": "np --no-yarn"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^22.4.2",
    "np": "^2.20.1"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "**/*.js",
      "!*.*"
    ],
    "coverageReporters": [
      "html"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?|jsx?)$"
  }
}
